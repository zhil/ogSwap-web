@import "./components/index.pcss";


.container {
  @media (min-width: theme('screens.xl')) {
    max-width: 1200px;
  }
}

.container-1440 {
  @apply container;

  @media (min-width: theme('screens.xl')) {
    max-width: 1440px;
  }
}

.underline-thickness-0.5 {
  text-decoration-thickness: 0.5px;
  text-underline-offset: 2px;
}
